<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
   <name>directory-rest</name>
   <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
   <version>1.0.12</version>
   <description>module.directory.rest.module.description</description>
   <documentation/>
   <installation/>
   <changes/>
   <user-guide/>
   <provider>module.directory.rest.module.provider</provider>
   <provider-url>http://lutece.paris.fr</provider-url>
   <icon-url>images/admin/skin/plugins/directory/directory.png</icon-url>
   <copyright>Copyright 2001-2010 Mairie de Paris</copyright>
   	<core-version-dependency>
		<min-core-version/>
		<max-core-version/>	
	</core-version-dependency>
	
	<!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->
   <db-pool-required>0</db-pool-required>
   
    <!-- filters -->
    <filters>
        <filter>
            <filter-name>directory-rest</filter-name>
            <url-pattern>/jsp/site/rest/directory/*</url-pattern>
            <filter-class>fr.paris.lutece.plugins.directory.modules.rest.DirectoryRestFilter</filter-class>
        </filter>
    </filters>
  
</plug-in>